{
	"info": {
		"_postman_id": "ee4952bb-7069-4396-8495-6077e8de5df9",
		"name": "Vetris Admin",
		"schema": "https://schema.getpostman.com/json/collection/v2.0.0/collection.json"
	},
	"item": [
		{
			"name": "login",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.globals.set(\"base_url\", \"localhost:9090\");"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"auth": {
					"type": "basic",
					"basic": {
						"username": "suresh",
						"password": "test123"
					}
				},
				"method": "POST",
				"header": [
					{
						"key": "Authorization",
						"value": "something",
						"type": "text",
						"disabled": true
					}
				],
				"url": "localhost:8888/vetris-security/v1/users/signon"
			},
			"response": []
		},
		{
			"name": "JWTdecode",
			"protocolProfileBehavior": {
				"disableBodyPruning": true
			},
			"request": {
				"method": "GET",
				"header": [
					{
						"key": "AUTHORIZATION",
						"value": "{{AUTHORIZATION}}",
						"type": "text"
					}
				],
				"body": {
					"mode": "raw",
					"raw": ""
				},
				"url": "http://localhost:8888/vetris-security/v1/users/decode"
			},
			"response": []
		}
	]
